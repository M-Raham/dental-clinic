<svg width="300" height="300" viewBox="0 0 300 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f0f9ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e0f2fe;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="skinGradient">
      <stop offset="0%" style="stop-color:#fbbf24;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f59e0b;stop-opacity:1" />
    </radialGradient>
  </defs>
  <rect width="300" height="300" fill="url(#bgGradient)"/>
  
  <!-- Professional portrait of Dr. Sarah Johnson -->
  <g transform="translate(150, 150)">
    <!-- Hair/Head silhouette -->
    <ellipse cx="0" cy="-20" rx="45" ry="50" fill="#8b4513"/>
    <path d="M -45 -30 Q -45 -60, -20 -65 Q 0 -70, 20 -65 Q 45 -60, 45 -30 Q 40 -25, 35 -30 Q 0 -40, -35 -30 Q -40 -25, -45 -30" fill="#654321"/>
    
    <!-- Face -->
    <ellipse cx="0" cy="-10" rx="38" ry="42" fill="url(#skinGradient)"/>
    
    <!-- Eyes -->
    <ellipse cx="-12" cy="-15" rx="4" ry="3" fill="#1f2937"/>
    <ellipse cx="12" cy="-15" rx="4" ry="3" fill="#1f2937"/>
    <ellipse cx="-11" cy="-16" rx="1.5" ry="1" fill="#ffffff"/>
    <ellipse cx="13" cy="-16" rx="1.5" ry="1" fill="#ffffff"/>
    
    <!-- Eyebrows -->
    <path d="M -18 -22 Q -12 -24, -6 -22" stroke="#654321" stroke-width="2" fill="none"/>
    <path d="M 6 -22 Q 12 -24, 18 -22" stroke="#654321" stroke-width="2" fill="none"/>
    
    <!-- Nose -->
    <path d="M 0 -10 L -3 0 L 3 0 Z" fill="#f59e0b" opacity="0.7"/>
    
    <!-- Mouth/Smile -->
    <path d="M -12 8 Q 0 15, 12 8" stroke="#dc2626" stroke-width="2" fill="none"/>
    <path d="M -10 10 Q 0 13, 10 10" fill="#fef3c7"/>
    
    <!-- Teeth -->
    <rect x="-8" y="10" width="4" height="3" rx="0.5" fill="#ffffff"/>
    <rect x="-2" y="10" width="4" height="3" rx="0.5" fill="#ffffff"/>
    <rect x="4" y="10" width="4" height="3" rx="0.5" fill="#ffffff"/>
    
    <!-- Lab coat -->
    <path d="M -50 30 Q -40 25, 0 25 Q 40 25, 50 30 L 50 120 L -50 120 Z" fill="#ffffff"/>
    <rect x="-50" y="30" width="100" height="90" fill="#f8fafc"/>
    
    <!-- Collar -->
    <path d="M -20 25 L -15 35 L 0 30 L 15 35 L 20 25" fill="#ffffff"/>
    
    <!-- Name tag -->
    <rect x="-25" y="50" width="50" height="20" rx="3" fill="#1e40af"/>
    <text x="0" y="63" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="10" font-weight="bold">DR. SARAH</text>
    
    <!-- Stethoscope -->
    <circle cx="20" cy="60" r="6" fill="#1e40af"/>
    <path d="M 20 54 Q 25 45, 30 40" stroke="#1e40af" stroke-width="3" fill="none"/>
    <circle cx="30" cy="40" r="4" fill="#60a5fa"/>
  </g>
  
  <!-- Professional lighting effect -->
  <ellipse cx="150" cy="100" rx="80" ry="40" fill="#ffffff" opacity="0.2"/>
</svg>
